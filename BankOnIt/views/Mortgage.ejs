            </table>
          </div>

          <div class="panel-heading">
            <h4 class="panel-title">
                   <img src="http://tradeoff-analytics-demo.mybluemix.net/images/app.png" alt="" style="width:25px; height:25px;">
              <a data-toggle="collapse" href="#collapse4">Savings Analytics</a>   <!--NAMING-->
            </h4>
          </div>

